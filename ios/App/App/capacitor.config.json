{
	"appId": "no.smartsauna.app",
	"appName": "Smart Sauna",
	"webDir": "dist",
	"server": {
		"url": "https://www.smartsauna.no",
		"cleartext": false,
		"allowNavigation": [
			"smartsauna.no",
			"*.smartsauna.no"
		]
	},
	"ios": {
